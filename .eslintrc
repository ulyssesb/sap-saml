{
    "env": {
        "node": true
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "no-unused-vars": "error",
        "no-undef": "error",
        "no-plusplus": "off"
    },
    "globals": {
        "describe": true,
        "it": true,
        "before": true,
        "Promise": true,
        "after": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2021
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"]
}
